<gconfentryfile>
  <entrylist base="/schemas/apps/guake">
    <entry>
      <key>general/compat_backspace</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
            <default_value>
              <value>
                <string>ascii-delete</string>
              </value>
            </default_value>
            <short>Backspace Compatibility</short>
            <long>Defines the behavior of backscape key.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/compat_delete</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
            <default_value>
              <value>
                <string>delete-sequence</string>
              </value>
            </default_value>
            <short>Delete Compatibility</short>
            <long>Defines the behavior of delete key.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/custom_command_file</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
            <default_value>
              <value>
                <string>~/.config/guake/custom_command.json</string>
              </value>
            </default_value>
            <short>Path to the default custom command json file.</short>
            <long>Path to the default custom command json file.
                If is blank or the json file is not in a proper format
                the terminal context menu will be built without custom commands.
                If the file is a valid json with recognized structure theterminak
                context menu will be buit with the commands read inside.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/debug_mode</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>false</bool>
              </value>
            </default_value>
            <short>Enable debug mode</short>
            <long>When debug mode is enabled, logs are printed in the
                standard output where Guake as been launched</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/default_shell</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
            <default_value>
              <value>
                <string></string>
              </value>
            </default_value>
            <short>Path to the default shell.</short>
            <long>Path to the default shell. Set to empty to
                use default user shell. If invalid path is set
                here, guake will fallback to user shell.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/display_n</key>
      <value>
        <schema>
          <type>int</type>
          <locale name="C">
            <default_value>
              <value>
                <int>0</int>
              </value>
            </default_value>
            <short>Display to appear on.</short>
            <long>Display to appear on if the  mouse_display option is not set.
                If this is set to an invalid value (as in the case of removing a
                screen from a system), the
                invalid value is automatically updated to the current primary
                screen.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/focus_if_open</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>true</bool>
              </value>
            </default_value>
            <short>Give focus to guake if tab is opened (OBSOLETE).</short>
            <long>If the guake window is out of focus but open,
                enabling this will give it back the focus instead of closing the window.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/history_size</key>
      <value>
        <schema>
          <type>int</type>
          <locale name="C">
            <default_value>
              <value>
                <int>1024</int>
              </value>
            </default_value>
            <short>History Size.</short>
            <long>The number of scrollback lines to save in
                history.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/max_tab_name_length</key>
      <value>
        <schema>
          <type>int</type>
          <locale name="C">
            <default_value>
              <value>
                <int>0</int>
              </value>
            </default_value>
            <short>Set max length of the tabs names</short>
            <long>Controls the maximum length of the tabs names. Maximum length: 100, Minimum length: 1. 0 = unlimited length.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/mouse_display</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>true</bool>
              </value>
            </default_value>
            <short>Appear on the mouse display.</short>
            <long>Appear on the mouse display. This overrides any setting
                in display_n.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/prompt_on_close_tab</key>
      <value>
        <schema>
          <type>int</type>
          <locale name="C">
            <default_value>
              <value>
                <int>0</int>
              </value>
            </default_value>
            <short>Prompt when closing tabs.</short>
            <long>0: Never 1: If process is running 2: Always. 
                Also prompts on quit.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/prompt_on_quit</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>true</bool>
              </value>
            </default_value>
            <short>Prompt when quitting.</short>
            <long>Ask for confirmation when quitting Guake.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/quick_open_command_line</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
            <default_value>
              <value>
                <string>gedit %(file_path)s</string>
              </value>
            </default_value>
            <short>Command line pattern to start a editor.</short>
            <long>This property gives the command line to execute when
                doing a Quick Open. You can enter the following python
                string pattern in it: &apos;%(file_path)s&apos; will be replaced
                with the full path to the captured filename,
                &apos;%(line_number)s&apos; will be replaced by the line number if
                found (set to the empty string otherwise).</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/quick_open_enable</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>false</bool>
              </value>
            </default_value>
            <short>Enable Quick Open feature.</short>
            <long>If this property is set as true, guake will
                open a text editor when the user does a Ctrl+Click
                on a filename printed in the terminal.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/quick_open_in_current_terminal</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>false</bool>
              </value>
            </default_value>
            <short>Quick open the file in the current terminal.</short>
            <long>If this property is set as true, guake will
                use the current terminal to execute the Quick open
                feature. Use if you want to inherit your terminal
                environment in your text editor</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/scroll_keystroke</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>true</bool>
              </value>
            </default_value>
            <short>Scroll on keystroke</short>
            <long>If true and a key is pressed, the terminal is
                scrolled to the bottom.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/scroll_output</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>false</bool>
              </value>
            </default_value>
            <short>Scroll on output</short>
            <long>If true, whenever there is new output in the
                terminal, it will scroll to the bottom.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/show_resizer</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>true</bool>
              </value>
            </default_value>
            <short>If guake must show a control to make resizing more handful</short>
            <long>When true, guake shows a resizer on its bottom to allow user
                resize guake.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/start_fullscreen</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>false</bool>
              </value>
            </default_value>
            <short>Start fullscreen.</short>
            <long>When true, the program will start in fullscreen mode.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/startup_script</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
            <default_value>
              <value>
                <string></string>
              </value>
            </default_value>
            <short>Startup script to execute when guake starts</short>
            <long>This property allows you to execute a startup bash
                script each time guake starts. You can configure your
                initial tab settings within this script. Please see
                the command line argument of guake (&apos;guake --help&apos;)
                for complete help.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/tab_ontop</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>false</bool>
              </value>
            </default_value>
            <short>Tab on top.</short>
            <long>Makes tab bar on top of the Guake window. Per default,
                 tabs appears bellow the terminal. Setting this to True will
                 move the tab on top of the terminal. Requires a restart.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/toolbar_visible_in_fullscreen</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>true</bool>
              </value>
            </default_value>
            <short>If the tabs bar must be visible in fullscreen</short>
            <long>Set it as false to enjoy guake&apos;s fullscreen.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/use_audible_bell</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>false</bool>
              </value>
            </default_value>
            <short>Audible bell</short>
            <long>If true, the system alert sound
                will be played on a bell character.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/use_default_font</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>true</bool>
              </value>
            </default_value>
            <short>Use default font</short>
            <long>If true, Guake will use the GNOME monospace
                default font.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/use_login_shell</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>false</bool>
              </value>
            </default_value>
            <short>Login shell</short>
            <long>If true, the commands in terminal will be
                executed in a login shell.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/use_palette_font_and_background_color</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>true</bool>
              </value>
            </default_value>
            <short>Use palette font and backgrount colors</short>
            <long>If true, the terminal font and background colors will use the value
                defined in the palette. If false, it will use the value defined by the user.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/use_popup</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>true</bool>
              </value>
            </default_value>
            <short>Sets the popup notifications visibility.</short>
            <long>When true, popup notifications will be shown
                when Guake starts.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/use_scrollbar</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>true</bool>
              </value>
            </default_value>
            <short>Sets the scrollbar visibility.</short>
            <long>When true, scrollbar is visible.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/use_trayicon</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>true</bool>
              </value>
            </default_value>
            <short>Sets the tray visibility.</short>
            <long>When true tray is visible.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/use_visible_bell</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>false</bool>
              </value>
            </default_value>
            <short>Visible bell</short>
            <long>If true, the terminal will flash
                on a bell character.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/use_vte_titles</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>true</bool>
              </value>
            </default_value>
            <short>Use terminal titles for tab naming</short>
            <long>Any terminal program can set the terminal&apos;s title via special escape sequences. Some shells (e.g. bash) display user&apos;s prompt there (though you can adjust that to any arbitrary text in bashrc or similar) and update it automatically as the prompt changes. So you can have easy automatically updating meaningful tab titles with this option turned on.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/window_halignment</key>
      <value>
        <schema>
          <type>int</type>
          <locale name="C">
            <default_value>
              <value>
                <int>0</int>
              </value>
            </default_value>
            <short>Window horizontal alignment.</short>
            <long>Place Guake at:
                  0: center, 1: left, 2: right</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/window_height_f</key>
      <value>
        <schema>
          <type>float</type>
          <locale name="C">
            <default_value>
              <value>
                <float>50</float>
              </value>
            </default_value>
            <short>Window height (float).</short>
            <long>Percent of the screen that will be used by guake
                  terminal vertically.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/window_losefocus</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>false</bool>
              </value>
            </default_value>
            <short>Determine if guake hides on lose focus.</short>
            <long>If this property is set as true, guake will be
                hidden when the main window lose the focus (when
                Alt+Tab be pressed for example).</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/window_ontop</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>true</bool>
              </value>
            </default_value>
            <short>Stay on top.</short>
            <long>Makes guake window be on top of all other windows in the
                    desktop.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/window_tabbar</key>
      <value>
        <schema>
          <type>bool</type>
          <locale name="C">
            <default_value>
              <value>
                <bool>true</bool>
              </value>
            </default_value>
            <short>The tab bar visibility.</short>
            <long>When true, the tab bar is visible.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/window_valignment</key>
      <value>
        <schema>
          <type>int</type>
          <locale name="C">
            <default_value>
              <value>
                <int>0</int>
              </value>
            </default_value>
            <short>Window vertical alignment.</short>
            <long>Place Guake at:
                  0: top, 1: bottom</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/window_width</key>
      <value>
        <schema>
          <type>int</type>
          <locale name="C">
            <default_value>
              <value>
                <int>100</int>
              </value>
            </default_value>
            <short>Window width.</short>
            <long>Percent of the screen that will be used by guake
                  terminal horizontally.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/window_width_f</key>
      <value>
        <schema>
          <type>float</type>
          <locale name="C">
            <default_value>
              <value>
                <float>100</float>
              </value>
            </default_value>
            <short>Window width (float).</short>
            <long>Percent of the screen that will be used by guake
                  terminal horizontally.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>general/word_chars</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
            <default_value>
              <value>
                <string>-A-Za-z0-9,./?%&amp;#:_~</string>
              </value>
            </default_value>
            <short>Characters that are considered &quot;part of a word&quot;</short>
            <long>When selecting text by word, sequences of these
                characters are considered single words. Ranges can be
                given as &quot;A-Z&quot;. Literal hyphen (not expressing a
                range) should be the first character given. </long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>keybindings/global/show_hide</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
            <default_value>
              <value>
                <string>F12</string>
              </value>
            </default_value>
            <short>Keybinding to show/hide guake.</short>
            <long>Global keybinding to allow user call guake from each
                place after it&apos;s opened without clicks.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>keybindings/local/clipboard_copy</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
            <default_value>
              <value>
                <string>&lt;Control&gt;&lt;Shift&gt;c</string>
              </value>
            </default_value>
            <short>Copy clipboard.</short>
            <long>Copy the clipboard content.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>keybindings/local/clipboard_paste</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
            <default_value>
              <value>
                <string>&lt;Control&gt;&lt;Shift&gt;v</string>
              </value>
            </default_value>
            <short>Paste clipboard.</short>
            <long>Paste the clipboard content.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>keybindings/local/close_tab</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
            <default_value>
              <value>
                <string>&lt;Control&gt;&lt;Shift&gt;w</string>
              </value>
            </default_value>
            <short>Closes the current tab.</short>
            <long>Accelerator to active function that closes the current selected tab.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>keybindings/local/decrease_height</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
            <default_value>
              <value>
                <string>&lt;Control&gt;Up</string>
              </value>
            </default_value>
            <short>Decrease height.</short>
            <long>Decrease the screen height.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>keybindings/local/decrease_transparency</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
            <default_value>
              <value>
                <string>&lt;Control&gt;Shift&gt;Down</string>
              </value>
            </default_value>
            <short>Decrease transparency.</short>
            <long>Decrease the terminal transparency.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>keybindings/local/increase_height</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
            <default_value>
              <value>
                <string>&lt;Control&gt;Down</string>
              </value>
            </default_value>
            <short>Increase height.</short>
            <long>Increase the screen height.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>keybindings/local/increase_transparency</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
            <default_value>
              <value>
                <string>&lt;Control&gt;Shift&gt;Up</string>
              </value>
            </default_value>
            <short>Increase transparency.</short>
            <long>Increase the terminal transparency.</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>keybindings/local/move_tab_left</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
            <default_value>
              <value>
                <string>&lt;Control&gt;Shift&gt;Page_Up</string>
              </value>
            </default_value>
            <short>Move left current tab</short>
            <long>Move the current tab to the left</long>
          </locale>
        </schema>
      </value>
    </entry>
    <entry>
      <key>keybindings/local/move_tab_right</key>
      <value>
        <schema>
          <type>string</type>
          <locale name="C">
